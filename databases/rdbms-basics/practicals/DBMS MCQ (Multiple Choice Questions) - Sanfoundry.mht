------MultipartBoundary--z2imJk2xh4NJRSxtsWs3g84VoKhC1yon9hbgYC2LLN----
Content-Type: text/css
Content-Transfer-Encoding: binary
Content-Location: https://www.google.com/cse/static/element/ff97a008b4153450/mobile+en.css

@charset "utf-8";

.gsc-results-wrapper-overlay { box-sizing: border-box; top: 5%; left: 2%; height: 90%; width: 96%; }

.gsc-above-wrapper-area { display: none; }

.gsc-tabsArea { margin: 12px 0px; overflow: auto hidden; white-space: nowrap; border: 1px solid rgb(223, 225, 229); border-radius: 8px; }

.gsc-tabsArea::-webkit-scrollbar { display: none; }

.gsc-tabHeader { font-size: 12px; width: 50%; border: none; border-radius: 0px; padding: 0px; }

.gsc-refinementsArea { overflow: auto hidden; white-space: nowrap; border: 1px solid rgb(223, 225, 229); border-radius: 8px; }

.gsc-refinementsArea::-webkit-scrollbar { display: none; }

.gsc-refinementsGradient { position: absolute; pointer-events: none; right: 1px; height: 31px; width: 31px; border-top-right-radius: 8px; border-bottom-right-radius: 8px; z-index: 1; }

.gsc-refinementHeader { font-size: 12px; }

.gsc-webResult .gsc-results { width: auto; }

.gsc-webResult .gsc-result { border-bottom: 1px solid rgb(223, 225, 229); word-break: break-word; }

.gsc-search-box-tools .gsc-search-box .gsc-input { padding: 0px; }

.gsc-input-box { padding-top: 6px; padding-bottom: 5px; border-top-left-radius: 8px; border-bottom-left-radius: 8px; }

.gsc-input-box table { height: 100%; }

.gsc-search-button-v2 { width: auto; padding: 11px 10px; margin: 0px; border-radius: 0px 8px 8px 0px; }

.gsc-search-button-v2 svg { height: auto; width: 16px; }

.gsc-completion-container { border-collapse: collapse; }

td.gssb_a { padding-top: 5px; padding-bottom: 5px; border: 1px solid rgb(223, 225, 229); }

.gsc-results .gsc-cursor-box { border: 1px solid rgb(223, 225, 229); border-radius: 8px; text-align: center; padding: 0px; }

.gsc-results .gsc-cursor { display: flex; align-items: center; }

.gsc-cursor-container-previous { flex-grow: 1; text-align: right; padding: 10px 0px; }

.gsc-cursor-container-next { flex-grow: 1; text-align: left; padding: 10px 0px; }

.gsc-cursor-next-page { display: inline; cursor: pointer; margin-left: 45px; }

.gsc-cursor-numbered-page { display: inline; }

.gsc-cursor-final-page { display: inline; margin-right: 45px; }

.gsc-cursor-chevron { cursor: pointer; margin: 0px 16px -1px; }

.gcsc-branding { clear: both; }

.gcsc-branding, .gcsc-branding-text:not(.gcsc-branding-text-name) { display: block; text-align: center; }

.gcsc-branding-text-name { font-size: 16px; }

.gs-promotion-image-box img.gs-promotion-image, .gs-web-image-box-portrait img.gs-image, .gs-web-image-box-landscape img.gs-image { object-fit: cover; width: 100px; height: 100px; max-width: 100px; max-height: 100px; }

.gs-promotion-image-box, .gs-image-box.gs-web-image-box.gs-web-image-box-portrait, .gs-image-box.gs-web-image-box.gs-web-image-box-landscape { float: right; margin-left: 10px; width: auto; height: auto; }

.gsc-imageResult > .gsc-expansionArea { column-count: 2; column-gap: 0px; display: block; position: relative; }

.gs-image-scalable { height: auto; width: 100%; }

.gsc-imageResult { width: 100%; }

.gs-mobilePreview { background: rgb(32, 33, 36); }

.gs-imagePreviewArea { top: 0px; z-index: 4000; height: 100%; }

.gs-imagePreview { width: calc(100% - 20px); height: auto; margin: 0px auto; padding: 50px 0px; position: static; }

.gs-imagePreview:not([src$=".svg"]) { width: calc(100% - 20px); height: auto; }

.gs-closeButton { position: absolute; right: 0px; fill: rgb(255, 255, 255); padding: 10px; box-sizing: content-box; }

.gs-previewSnippet { background: rgb(0, 0, 0); margin: 0px; padding: 10px; }

.gs-previewUrl, .gs-previewSize { color: rgb(204, 204, 204); margin: 5px 0px; }

.gs-previewTitle { font-size: 18px; color: rgb(255, 255, 255); }

.gs-previewVisit { display: inline-block; color: rgb(255, 255, 255); border: 1px solid; border-radius: 15px; padding: 8px 15px; margin: 5px 0px; text-decoration: none; }

.gcsc-find-more-on-google-root { text-align: center; }

.gcsc-more-maybe-branding-box { border: 1px solid rgb(223, 225, 229); border-radius: 18px; margin: 3px; padding: 8px; display: inline-block; }

.gcsc-find-more-on-google { display: inline-block; margin-left: 5px; margin-right: 5px; padding-top: 0px; padding-bottom: 1px; }

.gcsc-find-more-on-google-query { max-width: 170px; }
