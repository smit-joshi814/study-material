.gsc-context-box .gsc-facet-label { width: 65px; padding-left: 2px; text-decoration: underline; color: rgb(17, 85, 204); cursor: pointer; }

.gsc-context-box .gsc-chart { width: 32em; padding: 3px; border-left: 1px solid rgb(17, 85, 204); border-right: 1px solid rgb(17, 85, 204); }

.gsc-context-box .gsc-top { border-top: 1px solid rgb(17, 85, 204); }

.gsc-context-box .gsc-bottom { border-bottom: 1px solid rgb(17, 85, 204); }

.gsc-context-box .gsc-chart div { background: rgb(17, 85, 204); height: 9px; }

.gsc-context-box .gsc-facet-result { color: rgb(17, 85, 204); width: 30px; text-align: right; padding-right: 5px; }

.gs-webResult .gs-title .gs-title.gsc-usr-group-heading { color: rgb(17, 85, 204); cursor: pointer; }

.gs-webResult .gs-title .gs-title.gsc-usr-group-heading b { color: rgb(17, 85, 204); }

.gcsc-find-more-on-google { color: rgb(17, 85, 204); }

.gcsc-find-more-on-google-magnifier { fill: rgb(17, 85, 204); }
